generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id         String   @id @db.Uuid
  customerId String
  amount     Decimal  @db.Decimal(12, 2)
  status     String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
